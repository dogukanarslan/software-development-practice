<table class="table table-hover">
    <thead>
      <tr>
        <% data.header.forEach(title => { %>
            <th scope="col"><%= title %></th>
        <% }) %>
      </tr>
    </thead>
    <tbody>
      <% data.body.forEach(data => { %>
      <tr>
        <td><%= data.title %></td>
        <td><%= data.description %></td>
        <td><%= data.creator %></td>
        <td><a href="/pins/<%= data._id %>" class="btn btn-sm btn-primary">Visit</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>